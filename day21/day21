# Definisi class Student
class Student:
    # Constructor atau initializer
    def __init__(self, name, age):
        self.name = name
        self.age = age
    
    # Method untuk menampilkan informasi siswa
    def display_info(self):
        print(f"Name: {self.name}, Age: {self.age}")

# Membuat objek student1 dari class Student
student1 = Student("John Doe", 20)

# Memanggil method display_info untuk menampilkan informasi siswa
student1.display_info()

# Membuat objek student2 dari class Student
student2 = Student("Jane Smith", 22)

# Memanggil method display_info untuk menampilkan informasi siswa
student2.display_info()
